# Uncomment this line to define a global platform for your project
platform :ios, '9.0'
use_frameworks!

# インストールするpodをまとめた定数
def install_pods
    pod 'Alamofire'
    pod 'BubbleTransition'
    pod 'TapticEngine'
    pod 'Firebase/Core'
    pod 'Firebase/Firestore'
    # Pods for GRIT Timer
end

# 定数をinstall
target 'DQNRejector' do
    install_pods
end
